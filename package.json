{
  "name": "@codog/vue3-infinite-loading",
  "version": "0.0.2",
  "description": "An infinite scroll plugin for Vue.js",
  "main": "./lib/index.umd.js",
  "type": "module",
  "module": "./lib/index.mjs",
  "files": [
    "lib",
    "src"
  ],
  "author": {
    "name": "Allen",
    "email": "allenforuse@gmail.com"
  },
  "scripts": {
    "serve": "vite",
    "dev": "webpack-dev-server --config scripts/webpack.config.js --hot --info=false --port 8000",
    "build": "NODE_ENV=production webpack --config scripts/webpack.config.js -p --progress --hide-modules",
    "build:lib": "vite build -c vite.config.lib.js",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:deploy": "bash ./scripts/deploy_docs.sh",
    "lint": "eslint -f friendly --ext .js,.vue ./",
    "test": "BABEL_ENV=test karma start scripts/karma.conf.js",
    "precommit": "npm run lint",
    "commitmsg": "commitlint -E GIT_PARAMS",
    "release": "bash ./scripts/release.sh",
    "docs:preview": "vitepress preview docs"
  },
  "keywords": [
    "vue",
    "vue3",
    "vue components",
    "infinite loading",
    "infinite scroll",
    "vue infinite"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/justforuse/vue3-infinite-loading.git"
  },
  "bugs": {
    "url": "https://github.com/justforuse/vue3-infinite-loading/issues"
  },
  "homepage": "https://github.com/justforuse/vue3-infinite-loading",
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "@commitlint/cli": "^7.0.0",
    "@commitlint/config-conventional": "^7.0.1",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vitepress-demo-preview/component": "^2.3.2",
    "@vitepress-demo-preview/plugin": "^1.2.3",
    "@vuepress/plugin-google-analytics": "^1.0.0-alpha.16",
    "autoprefixer": "^9.1.5",
    "axios": "^1.7.7",
    "babel-loader": "^8.0.2",
    "babel-plugin-istanbul": "^5.0.1",
    "chai": "^3.5.0",
    "css-loader": "^0.28.4",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-formatter-friendly": "^6.0.0",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-vue": "^4.7.1",
    "focus-visible": "^4.1.5",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^0.14.3",
    "karma": "^3.0.0",
    "karma-chai": "^0.1.0",
    "karma-coverage": "^1.1.1",
    "karma-mocha": "^1.3.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-sinon-chai": "^1.3.1",
    "karma-spec-reporter": "0.0.31",
    "karma-webpack": "^4.0.0-rc.2",
    "less": "^3.8.1",
    "less-loader": "^4.1.0",
    "mocha": "^5.2.0",
    "phantomjs-prebuilt": "^2.1.15",
    "postcss-loader": "^3.0.0",
    "sass": "^1.80.1",
    "sinon": "^2.4.1",
    "sinon-chai": "^2.13.0",
    "tiny-emitter": "^2.1.0",
    "vite": "^5.4.9",
    "vitepress": "^1.4.1",
    "vue": "^3.2.0",
    "vue-loader": "^15.7.0",
    "vue-router": "^4.4.5",
    "vue-template-compiler": "^2.6.10",
    "vuepress": "^1.0.0-alpha.23",
    "webpack": "^4.17.2",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.8"
  },
  "peerDependencies": {
    "vue": "^3.2.0"
  },
  "license": "MIT",
  "browserslist": [
    "> 1%",
    "last 1 versions",
    "last 4 Android versions",
    "last 3 iOS versions"
  ],
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "scope-enum": [
        2,
        "always",
        [
          "core",
          "config",
          "spinner",
          "deps"
        ]
      ]
    }
  }
}
